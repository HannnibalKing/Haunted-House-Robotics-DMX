{
  "compilerOptions": {
    "target": "es2020",
  "module": "Node16",
    "moduleResolution": "node16",
    "jsx": "react-native",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom", "dom.iterable"],
  "types": ["jest", "react"]
  },
  "include": ["src/**/*", "App.tsx", "__tests__/**/*"]
}
